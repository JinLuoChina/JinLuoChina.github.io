# 图片缓存30天
/*.webp
/*.png
/*.jpg
  Cache-Control: public, max-age=2592000, immutable

# CSS/JS缓存1个月
/*.css
/*.js
  Cache-Control: public, max-age=2592000, immutable

# HTML不缓存（确保内容及时更新）
/*.html
  Cache-Control: no-cache

# 字体文件缓存1年
/*.woff2
/*.woff
/*.ttf
  Cache-Control: public, max-age=31536000, immutable
